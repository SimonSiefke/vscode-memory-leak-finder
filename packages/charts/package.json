{
  "name": "@vscode-memory-leak-finder/charts",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "dev": "node src/main.ts"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.vscode-test/"
    ],
    "injectGlobals": false,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "dependencies": {
    "@babel/code-frame": "^7.27.1",
    "@lvce-editor/rpc": "^5.1.0",
    "escape-html": "^1.0.3"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.27.0",
    "@types/escape-html": "^1.0.4"
  }
}
